	<field name="mati" class="java.lang.Long"/>
	<variable name="PertumbuhanPenduduk" class="java.lang.Double">
		<variableExpression><![CDATA[$F{lahir} - $F{mati} * 100/100]]></variableExpression>
	</variable>
